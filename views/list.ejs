<%- include('header'); -%>

    <div class="box" id="heading">
        <h1><%= listTitle %></h1>
    </div>
    <div class="box">
        <% for(item of newItems){%>
            <div class="item">
                <input type="checkbox">
                <p><%=item%></p>
            </div>
        <% } %>
        <form action="/" method="post">
            <input name='newItem' type="text" placeholder="New Item" autocomplete="off">
            <button type="submit" name='list' value=<%= listTitle %>>+</button>
        </form>
    </div>
        
    
<%- include('footer'); -%>
